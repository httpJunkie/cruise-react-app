{
  "name": "cruise-react-app",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "cross-fetch": "^2.2.2",
    "font-awesome": "^4.7.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-flexbox-grid": "^2.1.2",
    "react-fontawesome": "^1.6.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "remarkable": "^1.7.1",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.79.0"
  },
  "scripts": {
    "build:css": "postcss src/styles/app.css -o src/index.css",
    "watch:css": "postcss src/styles/app.css -o src/index.css -w",
    "start": "npm-run-all -p watch:css start-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build:css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.3",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^5.0.1",
    "postcss-import": "^11.1.0",
    "postcss-nested": "^3.0.0",
    "postcss-preset-env": "^5.1.0"
  }
}
